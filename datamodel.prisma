type Schedule {
  id: ID! @unique
  time: DateTime!
  user: User!
}

type SavedWorkout {
  id: ID! @unique
}

type Workout {
  id: ID! @unique
  name: String!
  schedule: Schedule!
}

type WorkoutExercises {
  id: ID! @unique
  workouts: [Workout!]!
  exercises: [Exercise!]!
}

type Exercise {
  sets: Int
  reps: Int
  intervals: Int
  duration: Int
  intensity: Int
}

type User {
  id: ID! @unique
  schedules: [Schedule!]!
  savedWorkouts: [SavedWorkout!]!
  notes: [Note!]!
  googleId: String!
}

type Note {
  id: ID! @unique
  note: String!
  workout: Workout!
  createdBy: User!
  schedule: Schedule!
}

type BodyMetric {
  weight: Float
  height: Float
  bodyfat: Float
  user: User!
}

type BodyMeasurement {
  hips: Float
  waist: Float
  rightArm: Float
  leftArm: Float
  rightLeg: Float
  leftLeg: Float
  user: User!
}